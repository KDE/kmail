<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.kmail.mailcomposer">
    <method name="send">
      <arg name="how" type="i" direction="in"/>
    </method>
    <method name="addAttachment">
      <arg name="url" type="s" direction="in"/>
      <arg name="comment" type="s" direction="in"/>
    </method>
    <method name="setBody">
      <arg name="body" type="s" direction="in"/>
    </method>
    <method name="addAttachment">
      <arg name="name" type="s" direction="in"/>
      <arg name="cte" type="s" direction="in"/>
      <arg name="data" type="ay" direction="in"/>
      <arg name="type" type="s" direction="in"/>
      <arg name="subType" type="s" direction="in"/>
      <arg name="paramAttr" type="s" direction="in"/>
      <arg name="paramValue" type="s" direction="in"/>
      <arg name="contDisp" type="s" direction="in"/>
    </method>
  </interface>
</node>
