        <receiver>warnCRLExpireCB</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
    <connection>
        <sender>useCRLsCB</sender>
        <signal>toggled(bool)</signal>
        <receiver>warnCRLExpireSB</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
    <connection>
        <sender>warnCRLExpireCB</sender>
        <signal>toggled(bool)</signal>
        <receiver>warnCRLExpireSB</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
</connections>
