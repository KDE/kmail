msgstr "Hiba az áthelyezés levélszűrő alkalmazásakor"

#: filtermanager.cpp:197
#, kde-format
msgid "Error applying mail filter delete"
msgstr "Hiba a törlés levélszűrő alkalmazásakor"

#: filtermanager.cpp:205
#, kde-format
msgid "Error applying mail filter modifications"
msgstr "Hiba a módosítások levélszűrő alkalmazásakor"

#: filtermanager.cpp:222
#, kde-format
msgid "<b>Evaluating filter rules:</b> "
msgstr "<b>Szűrési szabályok kiértékelése:</b> "

#: filtermanager.cpp:229
#, kde-format
msgid "<b>Filter rules have matched.</b>"
msgstr "<b>Szűrési szabályok illeszkedtek.</b>"

#: filtermanager.cpp:257
#, kde-format
msgid "<b>Begin filtering on message \"%1\" from \"%2\" at \"%3\" :</b>"
msgstr "<b>Szűrés kezdése a(z) „%1” üzeneten ettől: „%2” eddig: „%3” :</b>"

